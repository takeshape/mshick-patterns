  "mutations": {
    "upsertProduct": {
      "shape": "UpdateResult<Product>",
      "resolver": {
        "resultsMapping": {
          "debug_TakeshapeUpdate": [["get", {"path": "results.takeshapeUpdate"}]],
          "debug_TakeshapeCreate": [["get", {"path": "results.takeshapeCreate"}]],
          "debug_ShopifyProductUpdate": [["get", {"path": "results.shopifyProductUpdate"}]],
          "debug_ShopifyProductCreate": [["get", {"path": "results.shopifyProductCreate"}]],
          "result": [
            [
              "get",
              {
                "path": "results.finalTakeshapeResults"
              }
            ]
          ]
        },
        "compose": [
          {
            "if": "isEmpty(args.input._id) != true",
            "id": "takeshapeUpdate",
            "argsMapping": [
              [
                "get",
                {
                  "path": "args"
                }
              ]
            ],
            "name": "takeshape:update",
            "service": "local",
            "options": {
              "model": "Product"
            }
          },
          {
            "if": "isEmpty(args.input._id) == true",
            "id": "takeshapeCreate",
            "argsMapping": [
              [
                "get",
                {
                  "path": "args"
                }
              ]
            ],
            "name": "takeshape:create",
            "service": "local",
            "options": {
              "model": "Product"
            }
          },
          {
            "if": "isEmpty(get('takeshapeUpdate.result.shopifyProductId', results)) != true || isEmpty(get('takeshapeCreate.result.shopifyProductId', results)) != true",
            "id": "shopifyProductUpdate",
            "argsMapping": {
              "input.id": [
                [
                  "get",
                  {
                    "path": "results.takeshapeUpdate.result.shopifyProductId"
                  }
                ],
                [
                  "get",
                  {
                    "path": "results.takeshapeCreate.result.shopifyProductId"
                  }
                ]
              ],
              "input.title": [
                [
                  "get",
                  {
                    "path": "results.takeshapeUpdate.result.name"
                  }
                ],
                [
                  "get",
                  {
                    "path": "results.takeshapeCreate.result.name"
                  }
                ]
              ]
            },
            "name": "graphql:mutation",
            "service": "my-shopify-store",
            "options": {
              "fieldName": "productUpdate",
              "selectionSet": "{ product { id } }"
            }
          },
          {
            "if": "isEmpty(get('takeshapeCreate.result.shopifyProductId', results)) == true && isEmpty(get('takeshapeUpdate.result.shopifyProductId', results)) == true",
            "id": "shopifyProductCreate",
            "argsMapping": {
              "input.title": [
                [
                  "get",
                  {
                    "path": "results.takeshapeUpdate.result.name"
                  }
                ],
                [
                  "get",
                  {
                    "path": "results.takeshapeCreate.result.name"
                  }
                ]
              ]
            },
            "name": "graphql:mutation",
            "service": "my-shopify-store",
            "options": {
              "fieldName": "productCreate",
              "selectionSet": "{ product { id } }"
            }
          },
          {
            "if": "isEmpty(get('shopifyProductCreate.product.id', results)) != true",
            "argsMapping": {
              "input._id": [
                [
                  "get",
                  {
                    "path": "results.takeshapeCreate.result._id"
                  }
                ],
                [
                  "get",
                  {
                    "path": "results.takeshapeUpdate.result._id"
                  }
                ]
              ],
              "input.shopifyProductId": [
                [
                  "get",
                  {
                    "path": "results.shopifyProductCreate.product.id"
                  }
                ]
              ]
            },
            "name": "takeshape:update",
            "service": "local",
            "options": {
              "model": "Product"
            }
          },
          {
            "id": "finalTakeshapeResults",
            "argsMapping": {
              "_id": [
                [
                  "get",
                  {
                    "path": "results.takeshapeUpdate.result._id"
                  }
                ],
                [
                  "get",
                  {
                    "path": "results.takeshapeCreate.result._id"
                  }
                ]
              ]
            },
            "name": "takeshape:get",
            "service": "local",
            "options": {
              "model": "Product"
            }
          }
        ]
      },
      "description": "Update Product",
      "args": "UpdateArgs<Product>"
    }
  },
